<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>

<ToolBar xmlns:fx="http://javafx.com/fxml/1" fx:id="toolBar" stylesheets="@stylesheets/main.css"
         xmlns="http://javafx.com/javafx/8" fx:controller="cz.vianel.artwork.fx.EditToolBarController">
    <items>
        <Button styleClass="editButton" mnemonicParsing="false" text="Crop" onAction="#cropImage"/>
        <Button styleClass="editButton" mnemonicParsing="false" text="Rotate" onAction="#rotateImage"/>
        <Button styleClass="editButton" mnemonicParsing="false" text="Contrast/Brightness" onAction="#contrastBrightnessEdit"/>
    </items>
</ToolBar>
